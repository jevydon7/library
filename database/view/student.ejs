<h1>students routes works!</h1>
<table border="1">
  <tr>
    <th>id</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Username</th>
  </tr>
  <% students.forEach( (student) => { %>
  <tr>
    <td><%= student.id %></td>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.username %></td>
  </tr>
  <% }) %>
</table>
